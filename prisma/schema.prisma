// Prisma schema for local SQLite dev database
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Reservation {
  id        Int      @id @default(autoincrement())
  name      String
  email     String?
  phone     String
  date      String
  time      String
  service   String?
  message   String?
  createdAt DateTime @default(now())
}













